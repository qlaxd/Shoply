<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="StandardButtonBackground" Color="#0932c5"></SolidColorBrush>
    <SolidColorBrush x:Key="StandardButtonEnabled" Color="#04c1f5"></SolidColorBrush>
    <SolidColorBrush x:Key="StandardButtonNotEnabled" Color="#f44336"></SolidColorBrush>
    <SolidColorBrush x:Key="StandardButtonFocused" Color="#ffffff"></SolidColorBrush>
        <!--Background Colors-->
    <SolidColorBrush x:Key="backgroundPrimaryColor" Color="#04c1f5"/>
    <SolidColorBrush x:Key="backgroundSecondaryColor" Color="#0932c5"/>
    <SolidColorBrush x:Key="backgroundColorThird" Color="#1e122a"/>
    <Color x:Key="backgroundPrimaryColor1">#04c1f5</Color>
    <Color x:Key="backgroundPrimaryColor2">#0932c5</Color>
    <Color x:Key="backgroundPrimaryColor3">#1e122a</Color>
    <Color x:Key="backgroundSecondaryColor1">#04c1f5</Color>
    <Color x:Key="backgroundSecondaryColor2">#0932c5</Color>
    <Color x:Key="backgroundSecondaryColor3">#1e122a</Color>
    <!--Window Border Colors-->
    <Color x:Key="borderColor1">#04c1f5</Color>
    <Color x:Key="borderColor2">#ffffff</Color>
    <Color x:Key="borderColor3">#0932c5</Color>
    <Color x:Key="borderColor4">#1e122a</Color>
    <!-- -->
    <!--Color Palette-->
    <SolidColorBrush x:Key="colorRed" Color="#f44336"/>
    <SolidColorBrush x:Key="colorDarkRed" Color="#d32f2f"/>
    <SolidColorBrush x:Key="colorGreen" Color="#4caf50"/>
    <SolidColorBrush x:Key="colorMidleGreen" Color="#388e3c"/>
    <SolidColorBrush x:Key="colorDarkGreen" Color="#2e7d32"/>
    <!-- Control button color-->
    <SolidColorBrush x:Key="controlBackgraoundColor" Color="#0932c5"/>
    <SolidColorBrush x:Key="controlHoverColor" Color="#04c1f5"/>
    <!--Font Colors-->
    <SolidColorBrush x:Key="titleColor1" Color="#ffffff"/>
    <SolidColorBrush x:Key="titleColor2" Color="#ffffff"/>
    <SolidColorBrush x:Key="titleColor3" Color="#ffffff"/>
    <SolidColorBrush x:Key="plainTextColor1" Color="#ffffff"/>
    <SolidColorBrush x:Key="plainTextColor2" Color="#ffffff"/>
    <SolidColorBrush x:Key="plainTextColor3" Color="#ffffff"/>
    <!--Panel Colors-->
    <SolidColorBrush x:Key="panelColor" Color="#1e122a"/>
    <SolidColorBrush x:Key="panelOverColor" Color="#0932c5"/>
    <SolidColorBrush x:Key="panelActiveColor" Color="#04c1f5"/>
    <!--Button Colors-->
    <SolidColorBrush x:Key="ButtonBackgroundColor" Color="#04c1f5"/>
    <SolidColorBrush x:Key="ButtonHoverColor" Color="#0076f5"/>
    <SolidColorBrush x:Key="ButtonTextColor" Color="#ffffff"/>
    <!--TextBox Colors-->
    <SolidColorBrush x:Key="TextBoxBackgroundColor" Color="#3E5C6A"/>
    <SolidColorBrush x:Key="TextBoxBorderColor" Color="#0932c5"/>
    <SolidColorBrush x:Key="TextBoxTextColor" Color="#ffffff"/>
    <!--Panel Colors-->
    <SolidColorBrush x:Key="PanelBackgroundColor" Color="#1e122a"/>
    <SolidColorBrush x:Key="PanelBorderColor" Color="#0932c5"/>
    <SolidColorBrush x:Key="PanelTextColor" Color="#ffffff"/>
</ResourceDictionary>