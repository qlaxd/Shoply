<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:controlPanelViewModels="clr-namespace:ShoppingListAdmin.Desktop.ViewModels.ControlPanel"
                    xmlns:controlPanelView="clr-namespace:ShoppingListAdmin.Desktop.Views.ControlPanel"
                    xmlns:usersViewModels="clr-namespace:ShoppingListAdmin.Desktop.ViewModels.Users"
                    xmlns:usersView="clr-namespace:ShoppingListAdmin.Desktop.Views.Users"
                    >
    <DataTemplate DataType="{x:Type controlPanelViewModels:ControlPanelViewModel}">
        <controlPanelView:ControlPanelView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type usersViewModels:UsersViewModel}">
        <usersView:UsersView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type usersViewModels:ProductViewModel}">
        <usersView:ProductView/>
    </DataTemplate>
</ResourceDictionary>